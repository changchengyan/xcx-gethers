/**index.wxss**/
page{
  box-sizing: border-box;
  padding-bottom: 116rpx;
  overflow: auto;
}
.header{
  display: block;
  padding: 27rpx 30rpx 36rpx;
  font-size: 30rpx;
  line-height: 48rpx;
  background: #fff;
}
.xuanz {
  display:inline-block;
  width: 80rpx;
  height: 34rpx;
  margin: -5rpx 0 0;
  border-radius: 5rpx;
  background: #00bcd5;
  font-size: 24rpx;
  color: #fff;
  text-align: center;
  line-height: 36rpx;
  vertical-align: middle;  
}
.poster{
  display: block;
  padding: 0 30rpx;
  height: 348rpx;
  margin: auto;
  background: #fff;
}
.poster image,.poster video{
  width: 100%;
  height: 348rpx;
  background-color: #eeeeee;
}
.container{
 background: #fff;
}
.list{
  position: relative;
  padding: 27rpx 30rpx 27rpx 94rpx;
  border-bottom: 1px solid #eee;
  font-size: 28rpx;
  color: #333;
}
.list.error{
  padding-left: 94rpx;
  background:#fbfbfb url("data:image/png;base64,") no-repeat 30rpx center/40rpx 40rpx;
  color: #f46263;
}
.list.success{
  padding-left: 94rpx;
  background:#fff url("data:image/png;base64,") no-repeat 30rpx center/40rpx 40rpx;
  color: #6c0;
}
.list.error .number,.list.success .number{
  display: none;
}
.number{
  position: absolute;
  left: 30rpx;
  top: 50%;
  width: 40rpx;
  height: 40rpx;
  margin: -20rpx 0 0;
  border: 1rpx solid #f6f6f6;
  border-radius: 20rpx;
  text-align: center;
  line-height: 40rpx;
  font-size: 28rpx;
  font-weight: lighter;
  color: #666;
}
.list.active{
  background: #fbfbfb;
}
.list.active .number{
  background: #eee;
  color: #777;
}
.list.active-success{
  background: #fbfbfb;
  color: #54d16f;
}
.list.active-success .number{
  background: #54d16f;
  color: #fff;
}
.btn-bg{
  border-bottom: 1px solid #f6f6f6;
  background: #fff;
  overflow: hidden;
}
.btn{
  display: block;
  width: 690rpx;
  height: 80rpx;
  margin: 20rpx auto;
  background: #fed835;
  border-radius: 10rpx;
  text-align: center;
  font-size: 30rpx;
  color: #3d3d3c;
  line-height: 80rpx;
}
.analysis{
  margin: 20rpx 0 0;
  padding:25rpx 30rpx 40rpx;
  border-top: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
  background: #fff;
  font-size: 28rpx;
  line-height: 48rpx;
  color: #666;
}
.analysis .analysis-header{
  padding: 8rpx 0;
  font-size: 34rpx;
  color: #00bcd5;
}

.footer-layer{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.5);
}
.footer{
  position: fixed;
  box-sizing: border-box;
  bottom: 0;
  width: 100%;
  transform: translate3d(0, 100%, 0px);
  -webkit-transform: translate3d(0, 100%, 0px);
  z-index: 100;
}
.count{
  position: absolute;
  box-sizing: border-box;
  top: -93rpx;
  width: 100%;
  padding: 30rpx 30rpx;
  background: #fff;
  font-size: 24rpx;
  overflow: hidden;
  zoom:1;
  z-index:1000;
}
.count view{
  float: right;
  line-height: 34rpx;
}
.count .collect{
  float: left;
  height: 40rpx;
  padding-left: 45rpx;
  background: url("data:image/png;base64,") no-repeat left center/34rpx 34rpx;
  font-size: 24rpx;
  color: #666;
  line-height: 44rpx;
}
.count .collect.active{
  background: url("data:image/png;base64,") no-repeat left center/34rpx 34rpx;  
}
.count .remove{
  float: left;
  height: 40rpx;
  padding-left: 45rpx;
  margin-left: 20rpx;
  background: url("data:image/png;base64,") no-repeat left center/38rpx 38rpx;
  font-size: 24rpx;
  color: #666;
  line-height: 44rpx;
}
.count .page{
  height:40rpx;
  padding: 0 0 0 47rpx;
  font-size: 24rpx;
  color: #999;
  background: url("data:image/png;base64,") no-repeat left center/34rpx 34rpx;
  line-height: 44rpx;
}
.count .error{ 
  height:40rpx;
  margin-right: 30rpx;
  padding-left: 50rpx;
  background: url("data:image/png;base64,") no-repeat left center/40rpx 40rpx;
  font-size: 24rpx;
  color:#999;
  vertical-align: middle;
  line-height: 44rpx;
  }
.count .success{ 
  height:40rpx;
  margin-right: 30rpx;
  padding-left: 50rpx;
  background: url("data:image/png;base64,") no-repeat left center/40rpx 40rpx;
  font-size: 24rpx;
  color:#999;
  vertical-align: middle;
  line-height: 44rpx;
  }
.clear { zoom:1; overflow:hidden!important; }
.status-lists{
  max-height: 500rpx;
  padding: 0 0 24rpx;
  border-top: 1rpx solid #eaeaea;
  background: #fff;
  z-index: 100;
  overflow: auto;
}
.status-lists .status-list{
  float: left;
  width: 98rpx;
  height: 98rpx;
  margin: 24rpx 0 0 20rpx;
  border-radius: 50rpx;
  border: 1rpx solid #ccc;
  font-size: 30rpx;
  line-height: 98rpx;
  text-align: center;
  color: #ccc;
}
.status-lists .status-list.active{
  color:#999;
  border-color:#999;
  background: #f6f6f6;
}
.status-lists .status-list.error{
  color:#f46263;
  border-color:#f46263;
}
.status-lists .status-list.success{
  color:#54d16f;
  border-color:#54d16f;
}

.status-lists .status-list.error.active{
  color:#f46263;
  border-color:#f46263;
  background: #fff3f3;
}

.status-lists .status-list.success.active{
  color:#54d16f;
  border-color:#54d16f;
  background: #f7fff0;
}

/**swiper**/
page{
  height:100%;
}
.swiper-lists{
  position: relative;
  width: 100%;
  min-height:100%;
  overflow-x: hidden;
}
.swiper-lists .swiper-list{
  position: absolute;
  width: 100%;
  height:100%;
}
.swiper-lists .swiper-list.prev{
  top: 0;  
	transform: translate3d(-100%, 0 , 0px); 
  -webkit-transform: translate3d(-100%, 0 , 0px); 
}
.swiper-lists .swiper-list.next{
  top: 0;  
	transform: translate3d(100%, 0 , 0px); 
	-webkit-transform: translate3d(100%, 0 , 0px); 
}