.proloadBg {
  width: 0;
  height: 0;
  background: url(http://image.chubanyun.net/images/Dictation/word/bg_submit.png);
}

.no_more {
  display: block;
  width: 100%;
  padding: 10rpx 0;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  height: 40rpx;
  line-height: 40rpx;
}

._more {
  color: #fff;
  text-align: center;
  width: 100%;
  font-size: 28rpx;
  height: 40rpx;
  line-height: 40rpx;
  margin-top: -85%;
}
.sub_close {
