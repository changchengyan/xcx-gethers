/*富文本*/
text{word-wrap:break-word;word-break:break-all;}
.info-item .image{ padding: 10px 0px 10px 0px;}
.info-item  image{ max-width: 100%; width: 100%;}

/*提交弹框*/

.maskSubmit {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.85);
  z-index: 999999999999;
}

.maskSubmit .submitBox {
  position: relative;
  width: 600rpx;
  height: 446rpx;
  border-radius: 10rpx;
  background: #fff;
}

.maskSubmit .submitBox .top {
  background: url('http://image.chubanyun.net/images/Exam100/submit_bg1.png') no-repeat 0 0;
  background-size: cover;
  height: 120rpx;
  text-align: center;
  line-height: 120rpx;
  font-size: 36rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}

.maskSubmit .submitBox .text {
  text-align: center;
  font-size: 32rpx;
  line-height: 48rpx;
  color: #333;
  margin-top: 50rpx;
}

.maskSubmit .submitBox .btn {
  margin-top: 50rpx;
}

.maskSubmit .submitBox .btn view {
  width: 240rpx;
  height: 80rpx;
  line-height: 80rpx;
  box-sizing: border-box;
  font-size: 32rpx;
  text-align: center;
  border-radius: 10rpx;
}

.maskSubmit .submitBox .thinkAgin {
  border: 1px solid #91519d;
  margin-right: 40rpx;
}

.maskSubmit .submitBox .btn .hand {
  color: #fff;
}

.maskSubmit .submitBox .closeBox {
  position: absolute;
  right: -6rpx;
  top: -94rpx;
}

.maskSubmit .submitBox .closeBox .close {
  width: 54rpx;
  height: 54rpx;
  border: 1px solid #fff;
  border-radius: 50%;
}

.maskSubmit .submitBox .closeBox .close image {
  width: 28rpx;
  height: 28rpx;
  display: block;
}

.maskSubmit .submitBox .closeBox .line {
  width: 2rpx;
  height: 40rpx;
  background-color: #fff;
  margin: 0 auto;
}

/*支付弹框*/

.maskPay {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 999999999999;
}

.maskPay .payBox {
  position: relative;
  width: 600rpx;
  height: 799rpx;
  border-radius: 10rpx;
  background: #fff;
}

.payBox .payHeader {
  position: relative;
  height: 250rpx;
  background: url("http://image.chubanyun.net/images/Exam100/pay_bg.png") no-repeat 0 0;
  background-size: cover;
}

.payBox .payHeader .titleTop {
  justify-content: space-between;
}

.payBox .payHeader .isPaying {
  padding-left: 24rpx;
  font-size: 24rpx;
  color: #fff;
}

.payBox .payHeader .close {
  padding: 20rpx 24rpx;
}

.payBox .payHeader .close image {
  width: 29rpx;
  height: 29rpx;
  display: block;
}

.payBox .payHeader .showBook {
  position: absolute;
  left: 0;
  right: 0;
  margin: 0 auto;
  bottom: -54rpx;
  width: 212rpx;
  height: 212rpx;
  background: #fff;
}

.payBox .payHeader .showBook image {
  width: 200rpx;
  height: 200rpx;
  display: block;
}

.payBox .payContain {
  margin-top: 90rpx;
}

.payBox .payContain .bookName {
  width: 70%;
  height: 66rpx;
  margin: 0 auto;
  text-align: center;
  font-size: 24rpx;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.payBox .payContain .showMoney {
  font-size: 24rpx;
  color: #ff880a;
  align-items: flex-end;
  margin-top: 17rpx;
}

.payBox .payContain .disShowMoney {
  align-items: stretch;
}

.payBox .payContain .showMoney .dis_money {
  margin-right: 40rpx;
  align-items: flex-end;
}

.payBox .dis_original {
  justify-content: space-between;
}

.payBox .dis_original .dis_ori {
  color: #666;
}

.payBox .dis_original .dis_ori .disPrice {
  text-decoration: line-through;
}

.payBox .dis_original .disInfo {
  font-size: 20rpx;
  border: 1px solid #ff880a;
  padding: 0 8rpx;
  line-height: 28rpx;
  border-radius: 5rpx;
  max-width: 120rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.payBox .payContain .showNumOfMoney {
  font-size: 72rpx;
  line-height: 1;
  padding: 0 5rpx;
}

.payBox .payNow {
  width: 500rpx;
  height: 76rpx;
  line-height: 76rpx;
  margin: 0 auto;
  border-radius: 10rpx;
  background: #91509d;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
  margin-top: 80rpx;
}

.payBox .WXSecurit {
  font-size: 24rpx;
  color: #666;
  margin-top: 50rpx;
}

.payBox .WXSecurit image {
  width: 38rpx;
  height: 35rpx;
  display: block;
  margin-right: 5rpx;
}

.grade_list{
  margin-top: 48rpx
}

.grade_list .list_in {
  display: flex;
  align-items: center;
  flex-direction: row;
  width: 703rpx;
  height: 100%;
  margin: 0 auto;
  color: #fff;
  padding: 78rpx 36rpx;
  box-sizing: border-box;
  justify-content: space-between;
  border-radius: 15rpx;
  position: absolute;
  left:0;
  top:0;
}

.grade_list image {
  width: 45rpx;
  height: 80rpx;
}

.grade_list .out:nth-child(4n-3) {
  background-color: rgba(255, 204, 3, 0.9);
}

.grade_list .out:nth-child(4n-2) {
  background-color: rgba(255, 129, 90, 0.9);
}

.grade_list .out:nth-child(4n-1){
  background-color: rgba(44, 200, 248, 0.9);
}

.grade_list .out:nth-child(4n) {
  background-color: rgba(213,124,229, 0.9);
}

.end {
  padding: 0 20rpx;
  font-size: 24rpx;
  color: #666;
}
.end .header{
  justify-content: flex-start;
}
.end image {
  height: 320rpx;
  width: 100%;
  margin: 20rpx 0;
}

.bottom {
  position: fixed;
  width: 100%;
  height: 107rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  bottom: 0;
  left: 0;
  background-color: rgba(255, 255, 255, 0.9);
  box-shadow: 0 -2px 2px rgba(0, 0, 0, 0.1);
}

.bottom .buy {
  width: 500rpx;
  height: 78rpx;
  text-align: center;
  color: #fff;
  background-color: #91509d;
  line-height: 78rpx;
  border-radius: 10rpx;
}

.circle {
  position: absolute;
  background: linear-gradient(to right top, rgba(255, 255, 255, 0.9), rgba(255, 255, 255, 0));
  border-radius: 50%;
}

.circle:nth-child(1) {
  width: 20rpx;
  height: 20rpx;
  left: 40rpx;
  top: 225rpx;
}

.circle:nth-child(2) {
  width: 50rpx;
  height: 50rpx;
  left: 133rpx;
  top: 6rpx;
}

.circle:nth-child(3) {
  width: 30rpx;
  height: 30rpx;
  left: 390rpx;
  top: 70rpx;
  z-index: 9999;
}

.grade_list .bg {
  position: absolute;
  width: 300rpx !important;
  height: 100% !important;
  right: 0;
  border-top-right-radius: 15rpx;
  border-bottom-right-radius: 15rpx;
  opacity: 0.8;
  z-index: -222;
}

.grade_list .out {
  width: 703rpx;
  height: 262rpx;
  margin: 0 auto;
  color: #fff;
  box-sizing: border-box;
  border-radius: 15rpx;
  margin-bottom: 40rpx;
  position: relative;
  overflow: hidden;
}
.grade_list .out:nth-child(4n-3) .left_out{
background-image:linear-gradient(to right, rgba(255, 204, 3, .1) 80%, rgba(255, 204, 3, 1) 100%);
position:absolute;
right:305rpx;
width:60px;
height:100%;
box-shadow:30rpx 0 30rpx rgba(255, 204, 3, 1);
}
.grade_list .out:nth-child(4n-2) .left_out{
  background-image:linear-gradient(to right, rgba(255, 129, 90, .1) 80%, rgba(255, 129, 90, 1) 100%);
  position:absolute;
right:305rpx;
width:60px;
height:100%;
box-shadow:30rpx 0 30rpx rgba(255, 129, 90, 1);
}
.grade_list .out:nth-child(4n-1) .left_out{
  background-image:linear-gradient(to right, rgba(44, 200, 248, .1) 80%, rgba(44, 200, 248, 1) 100%);
  position:absolute;
right:305rpx;
width:60px;
height:100%;
box-shadow:30rpx 0 30rpx rgba(44, 200, 248, 1);
}
.grade_list .out:nth-child(4n) .left_out{
  background-image:linear-gradient(to right, rgba(213,124,229, .1) 80%, rgba(213,124,229, 1) 100%);
  position:absolute;
right:305rpx;
width:60px;
height:100%;
box-shadow:30rpx 0 30rpx rgba(213,124,229, 1);
}
.bg_out {
  width: 300rpx !important;
  height: 100% !important;
  overflow: hidden;
  position: absolute;
  right:0;
}
/*未授权登录的页面*/
.all_bg {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  position: fixed;
  z-index: 1000;
  background:#fff;
  top:0;
  left:0;
  right:0; 
}
.authorization_bg {
  width: 100%;
  position: absolute;
  top:0;
  bottom:0;
  box-sizing: border-box;
  padding: 0 40rpx;
  height:1045rpx;
  margin: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
  text-align: center;
}
.authorization_bg image {
  width: 370rpx;
  height: 400rpx;
}
.authorization_bg .word_tip {
  color:#888;
  font-size: 28rpx;
}
.authorization_bg .word_tip view{
  color:#333;
  padding-bottom: 42rpx;
  font-size: 40rpx;
}
.authorization_bg .word_tip text{
  display: block;
  padding-to: 6rpx;
}
.authorization_bg button {
  width: 100%;
  height: 90rpx;
  background-color: #91519d;
  color:#fff;
  font-size: 36rpx;
}