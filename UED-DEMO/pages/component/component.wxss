.app-list-canvas {
  background-color: #eee;
  padding: 20px;
}
.app-title{ font-size: 22px; text-align: center; padding: 0px 0px 20px 0px;}
.app-desc{ text-align: center; font-size: 12px; color: #777; padding: 0px 0px 20px 0px;}

.app-list-canvas .item{ padding: 15px; background-color: #fff; border-radius: 3px; font-size: 16px; margin-bottom: 10px;}
.app-list-canvas .item-disabled{ padding: 15px; background-color: #fff; border-radius: 3px; font-size: 16px; margin-bottom: 10px; color: #aaa;}

.app-list-canvas .more{float: right; font-size: 12px; color: #777; }